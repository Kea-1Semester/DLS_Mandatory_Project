{
  "GlobalConfiguration": {
    "BaseUrl": "http://localhost:8087",
    "RateLimitOptions": {
      "ClientIdHeader": "ClientId",
      "QuotaExceededMessage": "Too many requests, please try again later.",
      "HttpStatusCode": 429
    }
  },
  "Routes": [
    {
      "UpstreamPathTemplate": "/user",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/Placeholder/getstringFromUserService",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "userservice",
          "Port": 8082
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "Limit": 5
      }
    },
    {
      "UpstreamPathTemplate": "/auth",
      "UpstreamHttpMethod": [ "GET" ],
      "DownstreamPathTemplate": "/Placeholder/getstringFromAuthService",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "authservice",
          "Port": 8084
        }
      ],
      "RateLimitOptions": {
        "EnableRateLimiting": true,
        "Period": "1m",
        "Limit": 5
      }
    }
  ]
}
